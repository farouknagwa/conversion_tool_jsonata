      "n": 1,
      "stem": "<p>Put the steps of urine formation and excretion in the correct order.</p>",
      "type": "oq",
      "answer": null,
      "choices": [
        {
          "type": "item",
          "unit": null,
          "index": 1,
          "values": [],
          "last_order": false,
          "fixed_order": 3,
          "html_content": "Blood containing harmful nitrogenous wastes is carried to the kidneys."
        },
        {
          "type": "item",
          "unit": null,
          "index": 2,
          "values": [],
          "last_order": false,
          "fixed_order": 5,
          "html_content": "Wastes are filtered from the blood."
